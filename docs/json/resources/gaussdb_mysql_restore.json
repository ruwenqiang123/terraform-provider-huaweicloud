{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_gaussdb_mysql_restore": {
          "block": {
            "attributes": {
              "backup_id": {
                "type": "string",
                "description": "Specifies the ID of the backup to be restored.",
                "optional": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "region": {
                "type": "string"
              },
              "restore_time": {
                "type": "number",
                "description": "Specifies the time point of data restoration in the UNIX timestamp format.",
                "optional": true,
                "forcenew": true
              },
              "source_instance_id": {
                "type": "string",
                "description": "Specifies the source instance ID.",
                "required": true,
                "forcenew": true
              },
              "target_instance_id": {
                "type": "string",
                "description": "Specifies the target instance ID.",
                "required": true,
                "forcenew": true
              },
              "type": {
                "type": "string",
                "description": "Specifies the restoration type.",
                "required": true,
                "forcenew": true
              }
            }
          },
          "product": {
            "short": "GaussDBforMySQL",
            "name": "GaussDB(for MySQL)",
            "catalog": "Databases",
            "name_cn": "云数据库 GaussDB(for MySQL)",
            "catalog_cn": "数据库"
          }
        }
      }
    }
  }
}
